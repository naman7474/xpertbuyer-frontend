# XpertBuyer Frontend Environment Variables
# Single .env file for all environments

# Core API Configuration
REACT_APP_API_BASE_URL=https://backend-apis-gules.vercel.app/api
REACT_APP_VERSION=1.0.0
REACT_APP_ENVIRONMENT=production

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_DEBUG_MODE=false

# Analytics & Tracking
REACT_APP_GA_TRACKING_ID=
REACT_APP_SENTRY_DSN=

# Third-party Integrations
REACT_APP_YOUTUBE_API_KEY=

# App Configuration
REACT_APP_APP_NAME=XpertBuyer
REACT_APP_SUPPORT_EMAIL=support@xpertbuyer.com

# Development Overrides (uncomment for local dev)
# REACT_APP_API_BASE_URL=http://localhost:5000/api
# REACT_APP_ENABLE_DEBUG_MODE=true
